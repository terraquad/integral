{
  "schemaVersion": 1,
  "id": "integral",
  "version": "${version}",
  "name": "Integral",
  "description": "Server administration tool to verify that a client modpack hasn't been edited.",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/integral/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.terraquad.integral.client.IntegralClient"
      }
    ],
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.terraquad.integral.Integral"
      }
    ]
  },
  "mixins": [
    {
      "environment": "client",
      "config": "integral.client.mixins.json"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
  }
}
