{
  "schemaVersion": 1,
  "id": "integral",
  "version": "${version}",
  "name": "Integral",
  "description": "Anticheat utility to collect client modpack contents and edits",
  "authors": [
    {
      "name": "terraquad",
      "contact": {
        "email": "tq@terraquad.dev",
        "homepage": "https://terraquad.dev"
      }
    }
  ],
  "contact": {
    "sources": "https://github.com/terraquad/integral",
    "issues": "https://github.com/terraquad/integral/issues"
  },
  "license": "MIT",
  "icon": "assets/integral/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.terraquad.integral.client.IntegralClient"
      }
    ],
    "main": [
      {
        "adapter": "kotlin",
        "value": "dev.terraquad.integral.Integral"
      }
    ]
  },
  "mixins": [
    {
      "environment": "client",
      "config": "integral.client.mixins.json"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
  },
  "recommends": {
    "chatpatches": "*"
  }
}
